{
  "name": "@strongnguyen/nestjs-mssql",
  "version": "1.0.6",
  "description": "connection module for mssql",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register --recursive -R spec test/*.ts"
  },
  "keywords": [
    "nestjs-mssql"
  ],
  "author": "strongnguyen",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@types/mocha": "^5.2.7",
    "@types/mssql": "^7.1.5",
    "@types/node": "^14.0.0",
    "mocha": "^9.2.2",
    "mssql": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@quickts/nestjs-scanner": "^1.0.4"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strongnguyen29/nestjs-mssql-libs.git"
  }
}
